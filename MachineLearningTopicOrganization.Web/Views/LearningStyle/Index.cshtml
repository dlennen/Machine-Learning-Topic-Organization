@model IEnumerable<MLTO.Data.LearningStyleLookUp>

@{
    ViewBag.Title = "Index";
}

<h2>Learning Style Table</h2>

<@*p>
    @Html.ActionLink("Create New", "Create")
</p>*@
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.LearningStyle)
        </th>
        <th></th>
